{
    "name": "mazeltof/vectoricons",
    "type": "phpbb-extension",
    "description": "Replaces matrix icons with vectorial icons",
    "homepage": "https://www.phpbb.com/customise/db/extension/vector_icons/",
    "version": "1.2.5",
    "keywords": [
        "phpbb",
        "extension",
        "vectoricons"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "mazeltof",
            "homepage": "https://www.mazeland.fr/",
            "role": "Sorcerer's Apprentice"
        },
        {
            "name": "cabot",
            "homepage": "https://cabotweb.fr/",
            "role": "SuhiMaker"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Vector Icons",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.2"
        },
    "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/vector_icons",
            "filename": "version_check"
        }
    }
}
